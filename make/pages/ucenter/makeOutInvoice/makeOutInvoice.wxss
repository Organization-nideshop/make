page{
    height: 100%;
    width: 100%;
    background: #fff;
}
.container{
    background: #fff;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.icon{
    width: 400rpx;
    height: 400rpx;
    border-radius: 50%;
    border: 2rpx solid #eee;
    margin: 50rpx 0;
}
.orderId{
    width: 80%;
    display: flex;
    justify-content: center;
    border-bottom: 1px solid #d7d7d7;
    margin-bottom: 50rpx;
}
.defaultInfo{
    width: 100%;
    color: #999;
    text-align: center;
}

.btn-area{
    width: 80%;
    margin: 0 10%;
    color: #fff;
    background: #c9a16d;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 10rpx;
    font-size: 28rpx;
}