page{
    height: 100%;
    width: 100%;
    background: #fff;
}
.container{
    background: #fff;
    height: auto;
    overflow: hidden;
    width: 100%;
}
.birthInfo-list{
    background: #fff;
    display: flex;
    align-items: center;
    height: 100rpx;
    font-size: 28rpx;
    border-bottom: 2rpx solid #eee;
    margin: 0 5%;
}
.birthInfo-content{
    color: #999;
}
.birthInfo-title{
    width: 20%;
}
.button-bottom{
    color: #fff;
    background: #c9a16d;
    font-size: 28rpx;
    line-height: 80rpx;
    text-align: center;
    position: absolute;
    bottom: 0;
    width: 100%;
}