page{
    height: 100%;
    width: 100%;
    background: #fff;
}
.container{
    background: #fff;
    height: auto;
    overflow: hidden;
    width: 100%;
}
.birth-top image{
    width: 100%;
}
.birth-top{
    width: 100%;
}

.birthTips image{
    width: 100%;
    z-index: -5;
}
.birthTips{
    width: 100%;
    position: relative;
}
.tips-left{
    position: absolute;
    top: 20%;
    left: 5%;
    z-index: 9999;
}
.tips-left-top{
    color: #c9a16d;
    font-size: 32rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
}
.tips-left-top span{
    font-size: 28rpx;
}
.tips-left-bottom{
    font-size: 26rpx;
    color: #888;
}
.tips-right{
    position: absolute;
    bottom: 0;
    right: 5%;
    z-index: 9999;
}
.tips-right-top{
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #c9a16d;
}
.tips-right-top image{
    width: 20rpx;
    margin-right: 10rpx;
}
.tips-right-bottom{
    color: #fff;
    background: #c9a16d;
    border-radius: 20rpx;
    width: 120rpx;
    text-align: center;
    padding: 4rpx 0;
    margin: 10rpx auto;
    font-size: 26rpx;
}

.addBirthday{
    color: #fff;
    background: #c9a16d;
    font-size: 28rpx;
    line-height: 80rpx;
    text-align: center;
    position: absolute;
    bottom: 0;
    width: 100%;
}

.birthTips-none{
    text-align: center;
    font-size: 26rpx;
    color: #666;
    padding-top: 30rpx;
}