page {
  height: 100%;
  min-height: 100%;
  background: #fff;
  font-family: PingFang SC Medium;
}

.container {
  background: #fff;
  width: 100%;
  height: auto;
  min-height: 100%;
  overflow: hidden;
}

.no-cart {
  width: 100%;
  height: auto;
  margin: 0 auto;
}

.no-cart .c {
  width: 100%;
  height: auto;
  margin-top: 110rpx;
}

.no-cart .c image {
  margin: 0 auto;
  display: block;
  text-align: center;
  width: 170rpx;
  height: 200rpx;
}

.no-cart .c text {
  margin-top: 12rpx;
  display: block;
  /* width: 258rpx; */
  height: 29rpx;
  line-height: 29rpx;
  text-align: center;
  font-size: 29rpx;
  color: #999;
  letter-spacing: 2rpx;
}

.no-cart .c text.tip1 {
  color: #c9a16d;
}

.no-cart .c text.tip2 {
  color: #9e9e9e;
  font-size: 22rpx;
}

.cart-view {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.cart-view .list {
  height: auto;
  width: 100%;
  overflow: hidden;
  margin-bottom: 120rpx;
}

.cart-view .group-item {
  height: auto;
  width: 100%;
  background: #fff;
  margin-bottom: 18rpx;
}

.cart-view .item {
  height: 240rpx;
  width: 100%;
  overflow: hidden;
  padding: 0 20rpx;
  border-bottom: 8rpx solid #f4f4f4;
}

.cart-view .item .cart-goods {
  float: left;
  /* height: 164rpx; */
  height: 240rpx;
  width: 100%;
}

.cart-view .item .img {
  float: left;
  height: 200rpx;
  width: 210rpx;
  background: #f4f4f4;
  margin: 19.5rpx 18rpx 19.5rpx 0;
}

.cart-view .item .info {
  float: left;
  height: 180rpx;
  width: 450rpx;
  margin: 19.5rpx 26rpx 19.5rpx 0;
}

.cart-view .item .t {
  margin-bottom: 20rpx;
  height: 29rpx;
  color: #333;
  overflow: hidden;
}

.cart-goods .info .l {
  float: left;
  width: 340rpx;
  height: 130rpx;
}

.cart-goods .info .r {
  float: left;
  width: 110rpx;
  height: 130rpx;
}

.recycle {
  height: 80rpx;
}

.cart-goods .info .r image {
  height: 35rpx;
  width: 35rpx;
  float: right;
  margin-top: 30rpx;
}

.cart-view .item .t.type {
  height: 24rpx;
  margin: 0;
}

.cart-view .item .name {
  height: 30rpx;
  max-width: 310rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
}

.cart-view .item .num {
  height: 24rpx;
  line-height: 24rpx;
  float: right;
  color: #9e9e9e;
  font-size: 24rpx;
}

.cart-view .item .attr {
  margin-bottom: 17rpx;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: #9e9e9e;
  overflow: hidden;
}

.cart-view .item .b {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 25rpx;
  overflow: hidden;
}

.cart-view .item .price {
  float: left;
  height: 50rpx;
  line-height: 50rpx;
  color: #b99567;
  font-family: PingFang SC heavy;
  font-size: 30rpx;
}

.oldprice {
  float: left;
  height: 50rpx;
  line-height: 55rpx;
  color: #9e9e9e;
  font-family: PingFang SC heavy;
  font-size: 24rpx;
  text-decoration: line-through;
}

.cart-view .item .selnum {
  width: 110rpx;
  height: 50rpx;
  /* display: flex; */
  float: right;
  color: #b99567;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
}

.selnum .cut {
  width: 34rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  color: #b99567;
  border-radius: 34rpx;
  border: 1rpx solid #b99567;
}

.selnum .number {
  flex: 1;
  height: 100%;
  text-align: center;
  /* line-height: 68.75rpx; */
  float: left;
  color: #b99567;
  font-size: 30rpx;
}

.selnum .add {
  width: 34rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  color: #fff;
  background-color: #b99567;
  border-radius: 34rpx;
}

.cart-view .get {
  margin-top: 18rpx;
  float: right;
  height: 58rpx;
  padding-left: 14rpx;
  border-left: 1px solid #d9d9d9;
  line-height: 58rpx;
  font-size: 29rpx;
  color: #333;
}

.cart-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 100rpx;
  width: 100%;
  background: #fff;
  display: flex;
  border-top: 1px solid #c3c3c3;
}

.cart-bottom .total {
  height: 34rpx;
  /* flex: 1; */
  margin: 33rpx 20rpx 33rpx 50rpx;
  font-size: 29rpx;
  color: #b99567;
}

.cart-bottom .discount {
  height: 34rpx;
  flex: 1;
  margin: 36rpx 0;
  font-size: 26rpx;
  color: #9e9e9e;
}

.cart-bottom .delete {
  height: 34rpx;
  width: auto;
  margin: 33rpx 18rpx;
  font-size: 29rpx;
}

.cart-bottom .checkout {
  height: 100rpx;
  width: 300rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 29rpx;
  background: #b99567;
  color: #fff;
  font-family: PingFang SC Heavy;
}

/* 图标 */

#tips {
  background: #b99567;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  text-align: center;
  margin: 10rpx -5rpx;
}

.tips {
  background: #fff;
  color: #b99567;
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  position: absolute;
  display: block;
  margin: 10rpx -5rpx;
  font-size: 18rpx;
  line-height: 40rpx;
  text-align: center;
}

#tips:before, #tips:after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  height: 40rpx;
  width: 40rpx;
  background: #b99567;
}

#tips:before {
  -webkit-transform: rotate(30deg);
  -moz-transform: rotate(30deg);
  -ms-transform: rotate(30deg);
  -o-transform: rotate(30deg);
  transform: rotate(30deg);
}

#tips:after {
  -webkit-transform: rotate(60deg);
  -moz-transform: rotate(60deg);
  -ms-transform: rotate(60deg);
  -o-transform: rotate(60deg);
  transform: rotate(60deg);
}

.arrow-right {
  color: #999;
  width: 120rpx;
  float: right;
  box-sizing: border-box;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
}

.txt-more {
  width: 15rpx;
  height: 15rpx;
  border-right: 2rpx solid #999;
  border-bottom: 2rpx solid transparent;
  border-top: 2rpx solid #999;
  border-left: 2rpx solid transparent;
  transform: rotate(45deg);
}

.unit {
  font-size: 24rpx;
}

.navigator-hover {
  background-color: rgba(255, 255, 255, 1);
  opacity: 1;
}
